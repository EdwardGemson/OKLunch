<%= form_tag session_path,:class => "userform" do %>
<fieldset>
  <legend>Login</legend>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name, "",:class => 'userinput' %>
  </div>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password, "",:class => 'userinput' %>
  </div>
  <div>
    <%= submit_tag 'Log in', :class => 'usersubmit' %>
  </div>
</fieldset>
<% end %>
